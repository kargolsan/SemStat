<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="140.0" prefWidth="343.0" spacing="5.0" stylesheets="@../../Assets/Styles/Application/Bot.css" xmlns="http://javafx.com/javafx/8.0.91" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Application.Controllers.Application.BotController">
    <children>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <Label text="%tabs.bot.keyword" />
            <TextField id="keyword" fx:id="keyword" />
         </children>
      </HBox>
      <HBox alignment="TOP_CENTER" spacing="10.0">
         <children>
            <Button id="stop" fx:id="stop" disable="true" mnemonicParsing="false" onAction="#stop" text="%tabs.bot.break" />
            <Button id="analyze" fx:id="analyze" mnemonicParsing="false" onAction="#analyze" text="%tabs.bot.analyze" />
         </children>
         <VBox.margin>
            <Insets top="5.0" />
         </VBox.margin>
      </HBox>
    </children>
    <padding>
        <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
    </padding>
</VBox>
