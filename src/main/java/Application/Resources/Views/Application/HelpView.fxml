<VBox alignment="CENTER" prefHeight="442.0" prefWidth="596.0" spacing="5.0" stylesheets="@../../Assets/Styles/Application/Help.css" xmlns="http://javafx.com/javafx/8.0.91" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Application.Controllers.Application.HelpController">
