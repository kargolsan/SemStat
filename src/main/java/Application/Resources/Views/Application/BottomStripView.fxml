<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>

<HBox spacing="5.0" stylesheets="@../../Assets/Styles/Application/BottomStrip.css" xmlns="http://javafx.com/javafx/8.0.91" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Application.Controllers.Application.BottomStripController">
    <children>
      <Label text="%bottom_strip.status">
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </Label>
      <Label id="status" fx:id="status" text="%bottom_strip.waiting_commands" />
    </children>
    <padding>
        <Insets bottom="5.0" left="10.0" right="10.0" />
    </padding>
</HBox>
